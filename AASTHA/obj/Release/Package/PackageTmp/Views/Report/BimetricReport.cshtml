@model IEnumerable<AASTHA.Models.Biometric_Report_Result>
@{
    ViewBag.Title = "Manage_Patient";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<div class="blockui_rightgrid">
    <table class="table table-bordered">

        <thead>
            <tr>
                <th>
                    No.
                </th>
                <th>
                    Staff Name
                </th>
                <th>
                   Punching Date Time
                </th> 
                <th>
                    Punching In Time
                </th> 
                <th>
                    Punching Out Time
                </th>              
            </tr>
        </thead>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                   @item.Tran_MachineRawPunchId
                </td>
                <td>
                  @item.username
                </td>
                <td>
                   @item.PunchDatetime
                </td>              
            </tr>
        }

    </table>
</div>
